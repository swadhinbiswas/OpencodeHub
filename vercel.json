{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "regions": [
        "bom1"
    ],
    "env": {
        "GIT_REPOS_PATH": "/tmp/repos"
    },
    "build": {
        "env": {
            "GIT_REPOS_PATH": "/tmp/repos"
        }
    }
}